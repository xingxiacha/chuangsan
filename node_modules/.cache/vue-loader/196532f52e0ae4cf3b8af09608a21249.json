        left: 'center',\n          top: 'bottom',\n          data: ['类别']\n        },\n        radar: {\n          // shape: 'circle',\n          indicator: [\n            { name: '类别A', max: 10 },\n            { name: '类别B', max: 10 },\n            { name: '类别C', max: 10 },\n            { name: '类别D', max: 10 },\n            { name: '类别E', max: 10 },\n          ],\n          AxisName: { // (圆外的标签)雷达图每个指示器名称的配置项。\n            fontSize: 13,\n            color: 'black'\n          },\n        },\n        toolbox: {\n          feature: {\n            dataView: { readOnly: false },\n            restore: {},\n            saveAsImage: {}\n          }\n        },\n        series: [\n          {\n            name: 'Budget vs spending',\n            type: 'radar',\n            label: {  //显示value中具体的数值\n\n              show: true,\n              extStyle: {  //更改数值样式\n                color: 'blue'\n              }\n\n            },\n            data: [\n              {\n                value: [5, 8, 6, 7, 9],\n                name: '类别',\n                areaStyle: { color: \"#a9d4f6\" }, //阴影背景颜色\n                lineStyle: {\n                  color: \"#4890ff\",\n                  // 阴影折线宽度\n                  width: 1\n                },\n\n              }\n            ]\n          }\n        ]\n      })\n      window.addEventListener('resize', function () {\n        myChart.resize()\n      })\n    }\n  }\n}\n\n</script>\n\n\n\n"]}]}